# -----------------------------------------------
# INTEGERS TESTS
PTESTN1         : [[+ 1 2]]
POSTFIX         : 1 2 + .
ANSWER          : 3

PTESTN2         : [[- 2 1]]
POSTFIX         : 2 1 - .
ANSWER          : 1

PTESTN3         : [[/ 2 2]]
POSTFIX         : 2 2 / .
ANSWER          : 1

PTESTN4         : [[= 2 2]]
POSTFIX         : 2 2 = .
ANSWER          : -1

PTESTN5         : [[= 2 3]]
POSTFIX         : 2 3 = .
ANSWER          : 0

PTESTN6         : [[> 2 3]]
POSTFIX         : 2 3 > .
ANSWER          : 0

PTESTN7         : [[> 3 2]]
POSTFIX         : 3 2 > .
ANSWER          : -1

PTESTN8         : [[>= 2 3]]
POSTFIX         : 2 3 >= .
ANSWER          : 0

PTESTN9         : [[>= 3 2]]
POSTFIX         : 3 2 >= .
ANSWER          : -1

PTESTN10        : [[>= 3 3]]
POSTFIX         : 3 3 >= .
ANSWER          : -1

PTESTN11        : [[+ 1[* [+ 2 3] 7]]]
POSTFIX         : 1 2 3 + 7 * + .
ANSWER          : 36

PTESTN12        : [[+ 3 [* [- 4 3] [/ [% 2 2] 3]]]]
POSTFIX         : 3 4 3 - 2 2 mod 3 / * + .
ANSWER          : 3

PTESTN13        : [[and 2 2]]
POSTFIX         : 2 2 and .
ANSWER          : 2

PTESTN14        : [[and 2 3]]
POSTFIX         : 2 3 and .
ANSWER          : 2

PTESTN15        : [[or 2 2]]
POSTFIX         : 2 2 or .
ANSWER          : 2 

PTESTN16        : [[or 2 3]]
POSTFIX         : 2 3 or .
ANSWER          : 3

PTESTN17        : [[-1]]
POSTFIX         : 1 negate .
ANSWER          : -1

PTESTN18        : [[-2]]
POSTFIX         : 2 negate .
ANSWER          : -2

PTESTN19        : [[- [+ 1 2]]]
POSTFIX         : 1 2 + negate .
ANSWER          : -3

PTESTN20        : [[- [+ 2 [- 5]]]]
POSTFIX         : 2 5 negate + negate .
ANSWER          : 3

PTESTN21        : [[- [+ 2 [- 5 [- 4 3]]]]]
POSTFIX         : 2 5 4 3 - - + negate .
ANSWER          : -6

PTESTN22        : [[!= 1 1]]
POSTFIX         : 1 1 <> .
ANSWER          : 0

PTESTN23        : [[!= 1 2]]
POSTFIX         : 1 2 <> .
ANSWER          : -1

PTESTN24        : [[^ 2 2]]
POSTFIX         : 2 dup * .
ANSWER          : 4

PTESTN25        : [[^ 16 3]]
POSTFIX         : 16 dup dup * * .
ANSWER          : 4096

PTESTN26        : [[+ 2 [^ 2 4]]]
POSTFIX         : 2 2 dup dup dup * * * + .
ANSWER          : 18

PTESTN27        : [[+ 2 [- 3 [/ 4 [^ 16 3]]]]]
POSTFIX         : 2 3 4 16 dup dup * * / - + .
ANSWER          : 5

