ANSWER          : -6.

PTESTF20        : [[!= 1.0 1.0]]
POSTFIX         : 1.0e 1.0e F<> .
ANSWER          : 0

PTESTF21        : [[!= 1.0 2.0]]
POSTFIX         : 1.0e 2.0e F<> .
ANSWER          : -1

PTESTF22        : [[^ 2.0 2.0]]
POSTFIX         : 2.0e Fdup F* F.
ANSWER          : 4.

PTESTF23        : [[^ 16.0 3.0]]
POSTFIX         : 16.0e Fdup Fdup F* F* F.
ANSWER          : 4096.

PTESTF24        : [[+ 2.0 [ ^ 2.0 4.0]]]
POSTFIX         : 2.0e 2.0e Fdup Fdup Fdup F* F* F* F+ F.
ANSWER          : 18.

PTESTF25        : [[+ 2.0 [- 3.0 [/ 4.0 [^ 16.0 3.0]]]]]]
POSTFIX         : 2.0e 3.0e 4.0e 16.0e Fdup Fdup F* F* F/ F- F+ F.
ANSWER          : 4.9990234375
